{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "my-exoskeleton",
  "title": "My Exoskeleton",
  "description": "Development Tools",
  "icon": "icons/command-icon.png",
  "author": "Qlee",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "projects",
      "title": "Projects",
      "description": "All functions",
      "mode": "view"
    },
    {
      "name": "api-hub",
      "title": "API Hub",
      "description": "API Hub for project",
      "mode": "view"
    },
    {
      "name": "gocd-pipelines",
      "title": "GoCD Pipeline",
      "description": "GoCD Pipelines List And Details",
      "mode": "view",
      "icon": "icons/command-gocd-icon.png"
    },
    {
      "name": "grafana",
      "title": "Grafana",
      "description": "Grafana Quick Link",
      "mode": "view",
      "icon": "icons/command-grafana-icon.png"
    },
    {
      "name": "dictionary",
      "title": "Dictionary",
      "description": "Project Dictionary",
      "mode": "view",
      "icon": "icons/dictionary.png"
    },
    {
      "name": "ticket-library",
      "title": "Ticket Library",
      "description": "Operation Ticket Library",
      "mode": "view",
      "icon": "icons/ticket-library.png"
    },
    {
      "name": "bookmark",
      "title": "Bookmark",
      "description": "Project Bookmark",
      "mode": "view",
      "icon": "icons/bookmark.png",
      "preferences": []
    },
    {
      "name": "jira",
      "title": "JIRA",
      "description": "Jira Board Description",
      "mode": "view",
      "icon": "icons/jira-icon.png",
      "preferences": [
        {
          "name": "jiraBaseUrl",
          "title": "Jira board access base url",
          "description": "Jira board access base url",
          "placeholder": "Enter Your JIRA board access base url",
          "type": "textfield",
          "required": true
        },
        {
          "name": "jiraToken",
          "title": "Jira board access token",
          "description": "Jira board access token",
          "placeholder": "Enter Your JIRA board access key",
          "type": "textfield",
          "required": true
        }
      ]
    },
    {
      "name": "oauth",
      "title": "Oauth",
      "description": "Google Oauth",
      "mode": "view",
      "icon": "icons/google.ico"
    },
    {
      "name": "sync-configuration",
      "title": "Sync Configuration",
      "description": "Sync configuration from google sheet",
      "mode": "view",
      "icon": "icons/cloud-sync.png"
    }
  ],
  "preferences": [
    {
      "name": "configurationId",
      "title": "Configuration Center google sheet id",
      "description": "Bookmark google sheet ids",
      "placeholder": "Enter Your google sheet id",
      "type": "textfield",
      "required": true
    },
    {
      "name": "secondConfigurationId",
      "title": "Configuration Center google sheet id for yoursekf",
      "description": "Bookmark google sheet ids",
      "placeholder": "Enter Your google sheet id",
      "type": "textfield",
      "required": false
    },
    {
      "name": "oauthClientKey",
      "title": "google oauth client key",
      "description": "google oauth client key",
      "placeholder": "Enter Your oauth client key",
      "type": "textfield",
      "required": true
    }
  ],
  "dependencies": {
    "@mysql/xdevapi": "^8.0.32",
    "@raycast/api": "^1.47.3",
    "@raycast/utils": "^1.4.16",
    "@types/mysql": "^2.15.21",
    "@types/server-destroy": "^1.0.1",
    "axios": "^1.3.3",
    "csv-parse": "^5.3.5",
    "fs-extra": "^11.1.0",
    "google-auth-library": "^8.7.0",
    "googleapis": "^105.0.0",
    "moment": "^2.29.4",
    "mysql": "^2.18.1",
    "node-fetch": "^3.3.0",
    "node-notifier": "^10.0.1",
    "open": "^8.4.2",
    "server-destroy": "^1.0.1",
    "sqlite3": "^5.1.4",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.191",
    "@types/node": "18.8.3",
    "@types/node-notifier": "^8.0.2",
    "@types/react": "18.0.9",
    "@types/xlsx": "^0.0.36",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "lodash": "^4.17.21",
    "prettier": "^2.5.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist -o dist",
    "build:local": "ray build",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
